{
  "manifest_version": 3,
  "name": "Liquid Newtab",
  "version": "1.2.4",
  "description": "A clean and elegant customizable new tab page.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "search",
    "favicon"
  ],
  "background": {
    "service_worker": "background.js"
  }
}